AppPath=`dirname $0`

find -type f -name '*.h' | xargs --no-run-if-empty -l1 $AppPath/text-propset
find -type f -name '*.hpp' | xargs --no-run-if-empty -l1 $AppPath/text-propset
find -type f -name '*.cpp' | xargs --no-run-if-empty -l1 $AppPath/text-propset
find -type f -name '*.c' | xargs --no-run-if-empty -l1 $AppPath/text-propset
find -type f -name '*.dsp' | xargs --no-run-if-empty -l1 $AppPath/text-propset
find -type f -name '*.li' | xargs --no-run-if-empty -l1 $AppPath/text-propset
find -type f -name '*.li32' | xargs --no-run-if-empty -l1 $AppPath/text-propset
find -type f -name '*.li64' | xargs --no-run-if-empty -l1 $AppPath/text-propset
find -type f -name '*.vcproj' | xargs --no-run-if-empty -l1 $AppPath/text-propset
find -type f -name '*.def' | xargs --no-run-if-empty -l1 $AppPath/text-propset
find -type f -name '*.dll' | xargs --no-run-if-empty -l1 $AppPath/binary-propset
find -type f -name '*.so' | xargs --no-run-if-empty -l1 $AppPath/binary-propset
find -type f -name '*.lib' | xargs --no-run-if-empty -l1 $AppPath/binary-propset

