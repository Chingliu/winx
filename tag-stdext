if [ -z "$1" ] || [ -z "$2" ]; then
	echo "  Usage: tag-stdext <tag> <log-message>"
	echo
	exit 1
fi

tag=$1
tag_path=https://winx.googlecode.com/svn/tags/$tag
msg=$2

svn info $tag_path | grep "It is Not a valid URL"
errcode=$?
echo $errcode
if [ $errcode ];
then
	echo svn cp stdext $tag_path -m "$msg"
else
	echo "Tag $tag has exists"
fi

